extern void sleep(int)
extern int  printf(string)

func test_while_break(){
    a = 10
    while(a){
        printf("break\n")
        break
    }
}
func break_double(){
    b = 1
    while(b){
        c = 1
        printf("outside\n")
        while(c){
            printf("inside\n")
            break
        }
        break
    }
}

func main(){
    test_while_break()
    break_double()
}