package main

import fmt
import os

struct test
{
	i8 a
	u8 b
	i16 c
	u32 d
	i32 e
	u32 f
	i64 g
	u64 h
}
func i8_assign(p<test>){
	p.a = 100
	if(int(p.a) != 100){ 
		fmt.println("p.a != 100 ",int(p.a)) 
		os.exit(-1)
	}
	p.b = 100
	if(int(p.b) != 100){ 
		fmt.println("p.a != 100 ",int(p.b)) 
		os.exit(-1)
	}
	fmt.println("test i8_assign successful")
}
func main(){
	p<test> = new test
	
	i8_assign(p)
}